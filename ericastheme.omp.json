{
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#193549",
          "background": "#9700FF",
          "leading_diamond": "ÓÇ∂",
          "properties": {
            "postfix": " ",
            "prefix": "üéÉ ",
            "display_host": true,
            "display_user": false
          }
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#193549",
          "background": "#9700FF",
          "trailing_diamond": "ÓÇ∞",
          "properties": {
            "postfix": " ",
            "prefix": "‚ú® ",
            "ssh_icon": "",
            "display_host": false,
            "display_user": true
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#193549",
          "background": "#03AD00",
          "properties": {
            "display_stash_count": true,
            "display_upstream_icon": true,
            "status_colors_enabled": true,
            "local_changes_color": "#E36B00",
            "ahead_and_behind_color": "#f26d50",
            "behind_color": "#f17c37",
            "ahead_color": "#89d1dc",
            "stash_count_icon": "\uF692 "
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#111111",
          "background": "#ffff66",
          "properties": {
            "root_icon": "ÔÇ≠"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#193549",
          "background": "#868585",
          "properties": {
            "prefix": " Ôùä  ",
            "home_icon": "~",
            "folder_separator_icon": " \uE0b1 ",
            "style": "mixed",
            "max_depth": "3"
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#2e9599",
          "leading_diamond": "",
          "trailing_diamond": "ÓÇ¥",
          "properties": {
            "display_exit_code": false,
            "always_enabled": false,
            "error_color": "#f1184c",
            "color_background": true,
            "prefix": "<#83769c>ÓÇ∞</> Óà∫"
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#000000",
          "background": "#D4E157",
          "leading_diamond": "\uE0B6",
          "properties": {
            "postfix": " <#000000>\uE0B6</>"
          }
        },
        {
          "type": "go",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#7FD5EA",
          "leading_diamond": "\uE0B6",
          "display_version": true,
          "properties": {
            "postfix": " <#000000>\uE0B6</>"
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#111111",
          "background": "#2e9599",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "properties": {
            "time_format": "3:04:05 PM"
          }
        }
      ]
    }
  ],
  "final_space": true,
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}"
}

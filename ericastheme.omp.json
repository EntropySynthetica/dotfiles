{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#193549",
          "background": "#C76CCC",
          "leading_diamond": "\ue0b6",
          "template": "ðŸŒ™ {{ if .SSHSession }}\uf817 {{ end }}{{ .HostName }} ",
          "properties": {
            "display_host": true,
            "display_user": false
          }
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#193549",
          "background": "#C76CCC",
          "trailing_diamond": "\ue0b4",
          "template": "\u2728 {{ if .SSHSession }}{{ end }}{{ .UserName }} ",
          "properties": {
            "display_host": false,
            "display_user": true
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#193549",
          "background": "#89d1dc",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#E6B862{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#E6B862{{ end }}",
            "{{ if gt .Behind 0 }}#95FFB2{{ end }}"
          ],
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}}\uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#111111",
          "background": "#ffff66",
          "template": " \uf0e7 ",
          "properties": {
            "root_icon": "\uf0ad"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#193549",
          "background": "#868585",
          "template": " \uf07c {{ .Path }} ",
          "properties": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "max_depth": "3",
            "style": "mixed"
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#193549",
          "background": "#2e9599",
          "background_templates": [
            "{{ if gt .Code 0 }}#f1184c{{ end }}"
          ],
          "trailing_diamond": "\ue0b4",
          "template": " \uf00d{{ if gt .Code 0 }}\uf00d{{ else }}\uf42e{{ end }} ",
          "properties": {
            "always_enabled": false
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#193549",
          "background": "#D4E157",
          "leading_diamond": "\ue0b6",
          "template": "\uf820 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} <#000000>\ue0b6</>"
        },
        {
          "type": "go",
          "style": "diamond",
          "foreground": "#193549",
          "background": "#7FD5EA",
          "leading_diamond": "\ue0b6",
          "template": "Go {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} <#000000>\ue0b6</>"
        },
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#111111",
          "background": "#2e9599",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "3:04:05 PM"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#C76CCC",
          "template": " \ue285\ue285"
        }
      ],
      "newline": true
    }
  ]
}
